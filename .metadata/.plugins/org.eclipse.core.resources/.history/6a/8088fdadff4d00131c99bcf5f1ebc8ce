package nl.mprog.evilspacemonsterhangman.models;

public class EquivClass {
	/*
	 * class with a comparator to return 
	 */
	private int counter;
	private String currentWord;
	
	// constructor
	EquivClass(String equiv) {
		this.currentWord = equiv
		this.counter = 0;
	}
	
	public void increment() {
		this.counter++;
	}
}
